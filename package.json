{
  "name": "tic-tac-toe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=tic-tac-toe:* nodemon --inspect ./bin/www",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "postinstall": "sequelize db:migrate",
    "js:build": "webpack --config config/webpack.config.js",
    "js:dev": "webpack --watch --config config/webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [
      "public/**/**"
    ],
    "linters": {
      "*.{js,json}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.css": [
        "stylelint --fix",
        "prettier --write",
        "git add"
      ],
      "*.scss": [
        "stylelint --syntax=scss --fix",
        "prettier --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^8.5.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
