{
  "name": "tic-tac-toe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=tic-tac-toe:* nodemon ./bin/www",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "postinstall": "sequelize db:migrate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pg-promise": "^8.5.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0"
  }
}
